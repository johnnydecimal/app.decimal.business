{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@content/*": ["src/content/*"],
      "@helpers/*": ["src/helpers/*"],
      "@layouts/*": ["src/layouts/*"],
      "@styles/*": ["src/styles/*"]
    },
    "strictNullChecks": true
  },
  "exclude": ["src/**/_*", "dist/_astro"],
  "extends": "astro/tsconfigs/strict"
}
